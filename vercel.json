{
    "version": 2,
    "framework": null,
    "functions": {
        "api/index.php": { "runtime": "vercel-php@0.6.0" }
    },
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/api/index.php"
        }
    ],
    "env": {
        "APP_ENV": "production",
        "APP_DEBUG": "true",
        "APP_URL": "https://nama-project-anda.vercel.app",
        "APP_KEY": "base64:...", 
        "DB_CONNECTION": "mysql",
        "DB_HOST": "...",
        "DB_PORT": "4000",
        "DB_DATABASE": "test",
        "DB_USERNAME": "...",
        "DB_PASSWORD": "..."
    }
}